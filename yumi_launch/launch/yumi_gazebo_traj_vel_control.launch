<?xml version="1.0"?>
<launch>
  <arg name="debug" default="false"/>
  <!-- Load joint controller configurations from YAML file to parameter server -->
  <rosparam file="$(find yumi_control)/config/traj_controllers.yaml" command="load" ns="/yumi"/>
  <arg name="arm_controllers" default="joint_state_controller
                                       joint_traj_vel_controller_l
                                       joint_traj_vel_controller_r"/>

  <include file="$(find yumi_launch)/launch/yumi_gazebo_base.launch">
    <arg name="arm_controllers" value="$(arg arm_controllers)"/>
    <arg name="arms_hardware_interface" value="VelocityJointInterface"/>
    <arg name="debug" value="$(arg debug)"/>
  </include>
</launch>
